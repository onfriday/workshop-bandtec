<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WorkshopBandtecApp.Pages.Musicas.MusicaPage">
  <StackLayout Padding="5">
    <Entry Text="{Binding Trilha}" Placeholder="Trilha" />
    <Entry Text="{Binding Nome}" Placeholder="Nome" />
    <Entry Text="{Binding Duracao}" Placeholder="Duração" />
    <Button Text="Salvar" Command="{Binding SalvarCommand}" />
  </StackLayout>
</ContentPage>